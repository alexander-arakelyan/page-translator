{
  "compilerOptions": {
    "jsx": "react-jsx",
    "outDir": "./dist/",
    "module": "es6",
    "target": "es6",
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noImplicitThis": false,
    "alwaysStrict": false
  }
}
